<h1>New Appointment</h1>

<div class="mx-auto text-center">
    <%= link_to 'Cancel', service_appointments_url(@service), class: "btn btn-secondary" %>
</div>

<div class='container'>
  <% if session[:user_id] != @service.user.id %>
    <div>Not authorized to access resource</div>
  <% else %>
    <%= render 'form', appointment: @appointment %>
  <% end %>
</div>

